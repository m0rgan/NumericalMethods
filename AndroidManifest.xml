            <intent-filter>
                <action android:name="ReglaFalsa" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="NewtonActivity"
            android:label="Método de Newton" >
            <intent-filter>
                <action android:name="Newton" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="SecanteActivity"
            android:label="Método de la Secante" >
            <intent-filter>
                <action android:name="Secante" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>

</manifest>
